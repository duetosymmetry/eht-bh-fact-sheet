         RowBox[{
          RowBox[{"-", 
           FractionBox["L", 
            SuperscriptBox[
             RowBox[{"r", "[", "\[Phi]", "]"}], "2"]]}], 
          FractionBox["1", 
           SqrtBox[
            RowBox[{"1", "-", 
             RowBox[{
              SuperscriptBox["L", "2"], 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"r", "[", "\[Phi]", "]"}], "-", "2"}], ")"}], "/", 
               SuperscriptBox[
                RowBox[{"r", "[", "\[Phi]", "]"}], "3"]}]}]}]]]}]}], "/.", 
        RowBox[{"L", "\[Rule]", 
         SqrtBox["27"]}]}], ",", 
       RowBox[{
        RowBox[{"r", "[", "0", "]"}], "\[Equal]", "50"}]}], "}"}], ",", "r", 
     ",", 
     RowBox[{"{", 
      RowBox[{"\[Phi]", ",", "0", ",", 
